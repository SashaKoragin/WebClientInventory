{
   "compilerOptions": {
      "target": "es5",
      "module": "es2015",
      "moduleResolution": "node",
      "sourceMap": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "lib": [ "es2015", "dom" ],
      "noImplicitAny": true,
      "suppressImplicitAnyIndexErrors": true,
      "types": [ "node" , "jasmine" ],
    },
    "include": [
      "src/**/*"
  ],
    "exclude": [
      "./node_modules"
    ]
}